   "source": [
    "import gym\n",
    "from environment import Environment\n",
    "from train import Trainer\n",
    "from dqn import DQN\n",
    "\n",
    "env  = Environment(args)\n",
    "agent  = DQN (env,args)\n",
    "\n",
    "#Train Agent\n",
    "Trainer(agent).run()\n",
    "\n",
    "#Play the game\n",
    "env.gym.monitor.start(args.out,force=True)\n",
    "agent.play()\n",
    "env.gym.monitor.close()\n",
    "\n"
   ]
  }
